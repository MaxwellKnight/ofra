[{"/Users/maxwellknight/Desktop/web-projects/react-training/src/index.js":"1","/Users/maxwellknight/Desktop/web-projects/react-training/src/App.js":"2","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Navbar.js":"3","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/MainContent.js":"4","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Footer.js":"5","/Users/maxwellknight/Desktop/web-projects/react-training/src/data.js":"6","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/NavItem.js":"7","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Contact.js":"8"},{"size":255,"mtime":1654821428206,"results":"9","hashOfConfig":"10"},{"size":796,"mtime":1655667713491,"results":"11","hashOfConfig":"10"},{"size":1918,"mtime":1655652636420,"results":"12","hashOfConfig":"10"},{"size":9222,"mtime":1655630312240,"results":"13","hashOfConfig":"10"},{"size":362,"mtime":1655114997605,"results":"14","hashOfConfig":"10"},{"size":2436,"mtime":1655665957946,"results":"15","hashOfConfig":"10"},{"size":720,"mtime":1655667312804,"results":"16","hashOfConfig":"10"},{"size":321,"mtime":1655662965324,"results":"17","hashOfConfig":"10"},{"filePath":"18","messages":"19","suppressedMessages":"20","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"21"},"1498brq",{"filePath":"22","messages":"23","suppressedMessages":"24","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"25"},{"filePath":"26","messages":"27","suppressedMessages":"28","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"29"},{"filePath":"30","messages":"31","suppressedMessages":"32","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"33"},{"filePath":"34","messages":"35","suppressedMessages":"36","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"37","messages":"38","suppressedMessages":"39","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"40","messages":"41","suppressedMessages":"42","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"43"},{"filePath":"44","messages":"45","suppressedMessages":"46","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/maxwellknight/Desktop/web-projects/react-training/src/index.js",[],[],[],"/Users/maxwellknight/Desktop/web-projects/react-training/src/App.js",["47"],[],"import React, {useState} from \"react\"\n\nimport Navbar from './componenets/Navbar'\nimport MainContent from \"./componenets/MainContent\";\nimport './App.css'\nimport NavItem from \"./componenets/NavItem\"\nimport Footer from \"./componenets/Footer\";\n\n\nexport default function App() {\n\n  const [isHome , changeIsHome] = useState(true)\n  const [id , changeId] = useState(1)\n  const { data } = require('./data')\n\n  function toggleIsHome(isVisible, id){\n    changeIsHome(isVisible)\n    changeId(id)\n  }\n\n  return (\n    <div className=\"page-wrapper\">\n      <Navbar data={data} toggle={toggleIsHome}/>\n      <div className=\"container\">\n        {isHome ? <MainContent data={data} toggle={toggleIsHome} /> : <NavItem data={data[id]} toggle={toggleIsHome}/>}\n      </div>\n      {/* <Footer /> */}\n    </div>\n  );\n}\n","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Navbar.js",["48","49"],[],"import {React, useState} from \"react\"\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\"\nimport {  faBars, faWindowClose } from \"@fortawesome/free-solid-svg-icons\"\nimport '../css/navbar.css'\n\nexport default function Navbar(props){\n\n    const { data , toggle } = props\n    const [isMobile, changeIsMobile] = useState(false)\n\n    /* change isMobile  */\n    function toggleIsMobile(){\n        changeIsMobile(prevIsMobile => !prevIsMobile)\n    }\n    /**\n     * printNav is responsible for printing the navigation in relation\n     * to the viewport mobile/desktop\n     */\n    function printNav(){\n        return (\n            <ul className={isMobile ? \"nav-list-mobile\": \"navigation-list\"  }>\n                {isMobile ? <FontAwesomeIcon className=\"close-nav\" icon={faWindowClose} onClick={toggleIsMobile}/> : \"\"}\n                {data.map(item => {\n                    return (\n                        <li className={isMobile ?  \"nav-item-mobile\" : \"navigation-item\"} key={item.id} onClick={() => {\n                            \n                            changeIsMobile(false)\n                            return toggle(false, item.id)\n                        \n                        }}>{item.title}</li>)\n                })}\n            </ul>\n        )\n    }\n    function css(element, styleToApply){\n        for(const property in styleToApply)\n            element.style[property] = styleToApply[property]\n    }\n\n    /** this function handles the openening the of the nav menu */\n    function handleOnNavClick(){\n\n        return(\n            console.log(\"Menu Clicked\")\n        )\n    }\n    \n\n    return (\n        <nav className=\"navigation\">\n            <img className=\"navigation-logo\" src=\"../../images/mandala.png\" alt=\"logo\" onClick={() => toggle(true)} />\n            <FontAwesomeIcon onClick={(toggleIsMobile)} className=\"navigation-bar\" icon={faBars}/>\n            {printNav()}\n        </nav>\n    )\n}","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/MainContent.js",["50"],[],"import {React, useState} from 'react'\nimport Footer from './Footer'\nimport \"../css/mainContent.css\"\n\nexport default function MainContent (){\n\n    const [readMore, changeReadMore] = useState(false)\n    console.log(readMore)\n    const styles = {\n        height: readMore ? \"auto\" : \"300px\"\n    }\n\n    function toggleReadMore(){\n        changeReadMore(prevReadMore => !prevReadMore)\n    }\n    return (\n        <main className=\"main-content\">\n            <section className=\"main-hero\">\n                <img src=\"../images/background.png\" alt=\"\" className=\"main-image\"/>\n                <h2 className=\"main-title\">עופרה יחיאלי</h2>\n                <p className=\"main-text\">המיפגשים שלי עם אנשים בקבוצה וגם עם האדם, שאותו אני מלווה באופן פרטני,\nממחישים לי כל פעם מחדש את הכוח של נתינה וקבלה בתוך מרחב \nשל תנועה, שיח והקשבה - בו מגלה כל אחד את זכות הבחירה, כדי\nלשנות דפוסי-התנהגות כלפי המציאות, ואיך ניתן לשחרר את מה שחוסם \nולהפוך אותו למשהו חיובי ומשמעותי יותר.\nלפעמים צריך רק מישהו אחד שיקשיב, שיכיל, שיתמוך וישקף\nאת מה שבאמת קורה, לגלות את האור שבתוכינו ולסמוך.\nמוצאת את עצמי בהודיה כל פעם מחדש, על כך שזכיתי ביכולת לאפשר \nלאחר, לראות את האור שנמצא בתוכו ולדעת שיש לי חלק קטן בגילוי הזה, \nשמוביל לשינוי הרצוי.</p>\n\n                <aside className=\"main-about\" style={styles}>\n                    <img src=\"../images/portrait.jpeg\" alt=\"\" className=\"main-portrait\" />\n                    <p className=\"main-about-text\">\n                    שמי עופרה (אנגל) יחיאלי, ילידת 1955 הרצליה\nמזה 40 שנה מתגוררת בבאר-שבע\nאמא לשלושה ילדים, סבתא לנכדה \nבוחרת להשתדל כל יום להיות מאושרת עם בן-זוגה ובחיים בכלל \nעם העליות והמורדות שבדרך..\nבעברה חינכה מאות תלמידים בבתי-ספר יסודי. עבודתה נעשתה תוך שליחות\nובדרך חוויתית ויצירתית. תמיד ניסתה לגרום לתלמידיה ללמוד מתוך סקרנות, \nלקחת אחריות למעשיהם ולהאמין בעצמם. (ישנן מספר תלמידות, שכבר \nאמהות לילדים בעצמן, והיא בקשר איתן עד היום).\nמשנת 2006 עוסקת במודעות דרך גוף-נפש-רוח. מנחה, מדריכה ומלווה \nבאופן פרטני וקבוצתי ומתאימה את עבודתה לקשת רחבה של אוכלוסיות \nבקהילה עם קבוצות גיל שונות.\n\n** תירגול היוגה, שמלווה אותי מעל עשרים שנה, חושף אותי בכל פעם \n    לכוח המרפא של הנשימה, ועד כמה היא משמעותית בחיי היומיום.\n    תירגול המדיטציה והמיינדפולנס, מלמדים אותי שוב ושוב לקבל וגם \n    לשחרר דפוסים מן העבר, שכבר לא משרתים אותי ולהתחבר אל \n    הרצונות האמיתיים שלי ומהם לצמוח.\n    זו עבודה יומיומית ולא קלה. כמובן שגם אני חוטאת לפעמים, אך הודות\n    לנסיון הרב בתירגול, יודעת לקחת את אותה הנשימה, שמחזירה אותי\n    במהרה אל ההווה ומזכירה לעצמי, שכל מה שמגיע-מבורך,\n    וכל מה שמתעכב-מדוייק.\n\n** בין לבין משתדלת לתת מקום לתחביביי השונים כמו -\n     כתיבת ספרים (יצאו לאור 4 ספרי ילדים וכן 4 ספרונים מעוררי מחשבה)\n     כתיבת שירים ומנגינות\n     ציור באקריליק\n     חריזת תכשיטים (שרשראות ועגילים)\n                    </p>\n                </aside>\n            </section>\n            <p className=\"read-more\" onClick={toggleReadMore}> {readMore ? \"קרא פחות\" : \"קרא עוד\" }</p>\n\n            <article className=\"secondary-content\">\n                {/* <h2 className=\"secondary-title\">מושגים על קצה המזלג</h2> */}\n                <div className=\"flex\">\n                    <div className=\"secondary-container\">\n                        <h3 className=\"title\">מדיטציית מיינדפולנס (קשיבות)</h3>\n                        <p className=\"paragraph\">\n                        המחשבות בראשינו באות והולכות ..כמו רכבות.. לעולם הן לא תעלמנה מחיינו, הן חלק מאיתנו וטוב שכך.. רק מה, כשהן תופסות חלק גדול מהזמן שלנו ומתחילות לנהל אותנו,  אנחנו בעצם יוצאים מאיזון.. ואז אנחנו לא מרוכזים, עייפים, מתקשים להרדם, לעיתים מתוסכלים או עצבניים חיים בפחד או בדאגה- ואנחנו לא תמיד מבינים שהמחשבות, הם לא תמיד המציאות האמיתית... בתירגול המדיטציה לומדים לשים-לב, מתי מגיעות המחשבות הלא רצויות ואז להפנות את תשומת הלב לנשימות ולתחושות. להזיז את המחשבות בקלילות ובלי שיפוטיות. פשוט, לאט-לאט אנו מבינים, שאיננו רוצים שהמחשבות הטורדניות ינהלו את חיינו. ובמשך התירגולים , חווים תחושה עמוקה של שליטה עצמית בהכרה, בחושים ובריגושים, ומזהים את העוצמה שבתוכנו.\nהתרגול מאפשר לנו לצאת לחיי היומיום עם כלים להתמודדות עם ניהול רגשות כמו- \nחרדה, דיכאון, לחץ וכו'. ניתן ללמוד לפתח נוכחות פעילה דרך תרגילי מיינדפולנס, ניתן לשפר את איכות השינה, הזיכרון והריכוז ובכלל להגיע לתחושת מיקוד ואיזון המשולבים \nבשלווה, קלילות  ומצב רוח טוב עם הוקרת תודה על הקיים.\n**  חשוב לציין, שמחקרי-מוח מוכיחים, שדרך מדיטציה ניתן  \n     לטפח את המוח, הגוף והנפש לרמת תפקוד גבוהה ובריאה.\n                        </p>\n                    </div>\n\n                    <div className=\"secondary-container\">\n                        <h3 className=\"title\">יוגה</h3>\n                        <p className=\"paragraph\">\n                        משמעות המילה יוגה בסנסקריט- איחוד, חיבור.\n                 בתירגול היוגה ישנה הלימה בין התנוחה לבין הנשימה לבין התחושה.\n                 ישנן תנוחות (אסאנות) בעמידה, ישיבה ושכיבה, לכל תנוחה דגש תנועתי\n                 המופנה לאזורים שונים בגוף.\n                        </p>\n\n                        <ul className=\"yoga-list\">\n                            <li className=\"yoga-item\">שיפור יציבה</li>\n                            <li className=\"yoga-item\">הפחתת לחץ, חרדות, כעסים</li>\n                            <li className=\"yoga-item\">שיחרור מתחים פיזיים</li>\n                            <li className=\"yoga-item\">שיפור נשימה</li>\n                            <li className=\"yoga-item\">הארכה והגמשת שרירים</li>\n                            <li className=\"yoga-item\">חיזוק מפרקים ועוד...</li>\n                        </ul>\n\n                        <p className=\"paragraph\">\n                        התמדה בתירגול מביאה לחיזוק ושיפור המערכת החיסונית, לתחושת\n                חיוניות, שלוות-נפש ושמחת-חיים.\n                        </p>\n                    </div>\n                    <div className=\"secondary-container\">\n                        <h3 className=\"title\">נשימה</h3>\n                        <p className=\"paragraph\">\n                        במילים פשוטות -שאיפה ונשיפה.\n                            כמה היא מובנת מאליה בחיי היומיום שלנו.\n                            בתרגילי יוגה ומדיטציה, נחשפים לתירגולים חוויתיים.\n                            מקבלים כלים מעשיים והבנה של הידע המעשי והמרתק \n                            של תירגול הנשימה, ומגלים את החשיבות והקלילות שמכניסה\n                            הנשימה לחיינו, ברגע שמתחברים אליה ונותנים לה להוביל\n                            את התנועה בחיינו.\n                        </p>\n                    </div>\n                </div>\n\n            </article>\n\n        </main>\n    )\n}","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Footer.js",[],[],"/Users/maxwellknight/Desktop/web-projects/react-training/src/data.js",[],[],"/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/NavItem.js",["51"],[],"import {React, useState} from \"react\"\nimport \"../css/navItem.css\"\nimport Contact from '../componenets/Contact'\n\nexport default function NavItem(props){\n\n    function printList(list){\n        return(\n            <ul className=\"item-list\">\n                {list.map((item, index) => <li  key={index}>* {item} *</li>)}\n            </ul>\n        )\n    }\n\n    return(\n            <div className=\"main-content item\">\n                <h1 className=\"item-title\">{props.data.title}</h1>\n                <p className=\"item-main-text\">{props.data.mainText}</p>\n                {props.data.hasOwnProperty('list') ? printList(props.data.list) : \"\"}\n                {props.data.id === 6 ?  <Contact /> : \"\"}\n            </div>\n    )\n}","/Users/maxwellknight/Desktop/web-projects/react-training/src/componenets/Contact.js",[],[],{"ruleId":"52","severity":1,"message":"53","line":7,"column":8,"nodeType":"54","messageId":"55","endLine":7,"endColumn":14},{"ruleId":"52","severity":1,"message":"56","line":35,"column":14,"nodeType":"54","messageId":"55","endLine":35,"endColumn":17},{"ruleId":"52","severity":1,"message":"57","line":41,"column":14,"nodeType":"54","messageId":"55","endLine":41,"endColumn":30},{"ruleId":"52","severity":1,"message":"53","line":2,"column":8,"nodeType":"54","messageId":"55","endLine":2,"endColumn":14},{"ruleId":"52","severity":1,"message":"58","line":1,"column":16,"nodeType":"54","messageId":"55","endLine":1,"endColumn":24},"no-unused-vars","'Footer' is defined but never used.","Identifier","unusedVar","'css' is defined but never used.","'handleOnNavClick' is defined but never used.","'useState' is defined but never used."]